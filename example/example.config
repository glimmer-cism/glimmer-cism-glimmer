# configuration for the EISMINT-1 test-case
# moving margin

[EISMINT-1 moving margin]

[grid]
# grid sizes
ewn = 31
nsn = 31
upn = 11
dew = 50000
dns = 50000

[options]
temperature = 1
flow_law = 2
isostasy = 0
sliding_law = 4
marine_margin = 2
stress_calc = 2
evolution = 2
basal_water = 2
vertical_integration = 0

[time]
tend = 20000.
dt = 10.
ntem = 1.
nvel = 1.
niso = 1.

[parameters]
flow_factor = 1
geothermal = -42e-3

[CF default]
title: EISMINT-1 moving margin

[CF output]
name: example.nc
frequency: 1000
variables: thk uflx vflx bmlt temp uvel vvel wvel

