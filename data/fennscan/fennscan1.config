[EIS ELA]
ela_file = mb.data 

[EIS Temperature]
temp_file = s10.data

[grid]
! grid sizes
ewn = 141
nsn = 119
upn = 22
dew = 20000
dns = 20000

[options]
temperature = 1 
#flow_law = 0
isostasy = 0
#sliding_law = 4
marine_margin = 0
stress_calc = 0
evolution = 2

[time]
tstart = -120000.
tend =   -100000.
dt = 2.
ntem = 1.
nvel = 1.
niso = 1.

[parameters]
flow_factor = 1.

[default]
title: std FENNSCAN run
institution: University of Edinburgh
references: see PhD Magnus Hagdorn

[output]
name: fennscan.1000a.nc
frequency: 500
variables: thk uflx vflx acab artm flwa temp topg lsurf

[input]
name: europe.20km.nc
