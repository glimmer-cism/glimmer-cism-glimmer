[EIS ELA]
ela_file = mb.data 

[EIS Temperature]
temp_file = s10.data

[EIS SLC]
slc_file = specmap.data

[grid]
! grid sizes
ewn = 141
nsn = 119
upn = 22
dew = 20000
dns = 20000

[options]
temperature = 1 
flow_law = 0
isostasy = 2
basal_water = 2
sliding_law = 4
slip_coeff = 1
marine_margin = 0
stress_calc = 1
evolution = 2
topo_is_relaxed = 1

[time]
tstart = -120000.
tend =   0.
dt = 1.
ntem = 1.
nvel = 1.
niso = 1.

[parameters]
flow_factor = 3.

[default]
title: std FENNSCAN run
institution: University of Edinburgh
references: see PhD Magnus Hagdorn

[output]
name: fennscan.500a.nc
frequency: 500
variables: thk uflx vflx acab artm topg lsurf eus bmlt slc

[output]
name: fennscan.2000a.nc
frequency: 2000
variables: thk temp topg eus uvel vvel wvel

[input]
name: europe.20km.nc
