#[]
#dimensions:    time, level, y1, x1
#units:         
#long_name:     
#data:          
#factor:        

# setup for code generator
[VARSET]
# prefix of the generated module
name:     glint
# f90 type containing all necessary data
datatype: glint_instance
# module where type is defined
datamod:  glint_type 

[mask]
dimensions:    time, y1, x1
units:         1
long_name:     upscaling and downscaling mask
data:          data%out_mask
load:          1
hot:           1
coordinates:   lon lat

[glint_acab]
dimensions:    time, y1, x1
units:         meter/year
long_name:     mass-balance
data:          data%acab_save
coordinates:   lon lat

[glint_artm]
dimensions:    time, y1, x1
units:         degreeC
long_name:     air temperture
data:          data%artm_save
coordinates:   lon lat

[glint_prcp]
dimensions:    time, y1, x1
units:         meter/year
long_name:     precipitation
data:          data%prcp_save
standard_name: lwe_precipitation_rate
coordinates:   lon lat

[glint_ablt]
dimensions:    time, y1, x1
units:         meter/year
long_name:     ablation
data:          data%ablt_save
coordinates:   lon lat

[global_orog]
dimensions:    time, y1, x1
units:	       meter
long_name:     orographic elevation provided by global model
data:	       data%global_orog
coordinates:   lon lat

[local_orog]
dimensions:    time, y1, x1
units:	       meter
long_name:     orographic elevation provided by global model
data:	       data%local_orog
coordinates:   lon lat
