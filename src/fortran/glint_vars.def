#[]
#dimensions:    time, level, y1, x1
#units:         
#long_name:     
#data:          
#factor:        

# setup for code generator
[VARSET]
# prefix of the generated module
name:     glint
# f90 type containing all necessary data
datatype: glint_instance
# module where type is defined
datamod:  glint_type 

[mask]
dimensions:    time, y1, x1
units:         1
long_name:     upscaling and downscaling mask
data:          data%out_mask
load:          1
hot:           1
coordinates:   lon lat

[arng]
dimensions:    time, y1, x1
units:         degree_Celsius
long_name:     annual temperature range
data:          data%arng
coordinates:   lon lat

[prcp]
dimensions:    time, y1, x1
units:         meter/year
long_name:     precipitation
data:          data%prcp
standard_name: lwe_precipitation_rate
coordinates:   lon lat

[ablt]
dimensions:    time, y1, x1
units:         meter/year
long_name:     ablation
data:          data%ablt
coordinates:   lon lat

[xwind]
dimensions:    time, y1, x1
units:	       meter/second
long_name:     westerly surface wind component
data:	       data%xwind
coordinates:   lon lat

[ywind]
dimensions:    time, y1, x1
units:	       meter/second
long_name:     southerly surface wind component
data:	       data%ywind
coordinates:   lon lat

[global_orog]
dimensions:    time, y1, x1
units:	       meter
long_name:     orographic elevation provided by global model
data:	       data%global_orog
coordinates:   lon lat

